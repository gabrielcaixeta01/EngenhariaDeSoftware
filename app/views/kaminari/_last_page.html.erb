<% disabled = current_page.last? %>
<li class="page-item <%= 'disabled' if disabled %>">
  <% label = raw(t('views.pagination.last', default: 'Last Â»')) %>
  <% if disabled %>
    <span class="page-link" aria-hidden="true"><%= label %></span>
  <% else %>
    <%= link_to label, url, class: "page-link", aria: { label: "Last" } %>
  <% end %>
</li>
